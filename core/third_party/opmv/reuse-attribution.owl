xmlns:xml="http://www.w3.org/XML/1998/namespace"
ontologyIRI="http://purl.org/net/opmv/types/attribution#">
<Prefix name="rdf" IRI="http://www.w3.org/1999/02/22-rdf-syntax-ns#"/>
<Prefix name="rdfs" IRI="http://www.w3.org/2000/01/rdf-schema#"/>
<Prefix name="xsd" IRI="http://www.w3.org/2001/XMLSchema#"/>
<Prefix name="owl" IRI="http://www.w3.org/2002/07/owl#"/>
<Import>http://purl.org/net/opmv/ns#</Import>
<Declaration>
<ObjectProperty IRI="reliedUpon"/>
</Declaration>
<Declaration>
<ObjectProperty IRI="reused"/>
</Declaration>
<Declaration>
<NamedIndividual IRI="http://purl.org/net/opmv/types/attribution"/>
</Declaration>
<SubObjectPropertyOf>
<ObjectProperty IRI="reliedUpon"/>
<ObjectProperty IRI="http://purl.org/net/opmv/ns#wasTriggeredBy"/>
</SubObjectPropertyOf>
<SubObjectPropertyOf>
<ObjectProperty IRI="reused"/>
